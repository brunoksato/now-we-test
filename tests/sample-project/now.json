{
  "version": 2,
  "builds": [
    {
      "src": "lambdas/*.js",
      "use": "@now/node"
    },
    {
      "src": "lambda.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/lambdas/index.js"
    },
    {
      "src": "/(?<id>.*)",
      "dest": "/lambdas/$id"
    }
  ]
}
